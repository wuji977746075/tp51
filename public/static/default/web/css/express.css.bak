body { margin: 0; padding: 0 }
.fr { float: right }
/*common*/
.smart-header { padding: 10px 0; border: none; background: #4690cd }
.smart-header .logo { margin: 0 auto; width: 100%; height: 3rem; line-height: 70px; text-indent: -65535px; overflow: hidden; background: url("//cdn.kuaidi100.com/images/smart/logo.png") center center no-repeat;background-size: 40%;}
.smart-header .title { margin: 0 auto; height: 40px; line-height: 40px; font-size: 30px; text-shadow: none; }
.smart-header .smart-return-btn { top: 15px; padding-left: 5px; width: 55px; height: 30px; line-height: 30px; text-shadow: none; box-shadow: none; border: 0px; border-radius: 0px; color: #4690cd; background: url("//cdn.kuaidi100.com/images/smart/icon.png") 0 -100px no-repeat }
.smart-header .smart-return-btn span { font-size: 14px; border: 0px; border-radius: 0px; }
.smart-header .smart-return-btn span.ui-btn-inner { padding: 0 11px; }
.smart-footer { height: 3rem; line-height: 30px; border: none; background: #fafafa; }
.smart-footer p { margin: 0; padding: 0 3px; height: 30px; line-height: 30px; vertical-align: middle; font-size: 16px; font-weight: normal; text-shadow: none; color: #8c8c8c }
.smart-footer p .smart-footer-a { margin: 0 4px; text-decoration: none; font-weight: normal; color: #8c8c8c }
.smart-footer p .smart-footer-a-dis { color: #333333 }
/*index*/
.smart-input-div { margin: 0 auto; padding: 5px 15px; width: 80%; max-width: 420px; }
.ui-content .input1 { margin: 20px auto 0; padding: 10px 15px; height: 20px; line-height: 20px; width: 80%; box-shadow: 2px 2px 3px #AAA; background: #FFFFFF; border: 2px solid #c4c4c4; }
.ui-content .smart-div-valid { margin: 20px auto 0; width: 250px; height: 40px; }
.ui-content .smart-div-valid img { display: inline-block; width: 80px; height: 40px; vertical-align: middle; border: 2px solid #c4c4c4; border-radius: .6em; -moz-border-radius: .6em; -o-border-radius: .6em; -webkit-border-radius: .6em; box-shadow: 2px 2px 3px #AAA; }
.ui-content .input2 { display: inline-block; width: 100px; margin-left: 15px; margin-right: 10px; padding: 10px 15px; height: 20px; line-height: 20px; vertical-align: middle; box-shadow: 2px 2px 3px #AAA; background: #FFFFFF; border: 2px solid #c4c4c4; }
.ui-content .input-focus { border: 2px solid #64a5da; }
.ui-content input[readonly].input1 { background: url("//cdn.kuaidi100.com/images/smart/icon.png") right -40px no-repeat #FFFFFF }
.ui-content .ui-btn-corner-all { padding: 0; box-shadow: none; border: 0px; border-radius: 0; -moz-border-radius: 0; -o-border-radius: 0; -webkit-border-radius: 0; }
.ui-content .a-query { margin: 25px auto; padding: 10px 10px 10px 0; width: 130px; height: 30px; line-height: 30px; font-size: 24px; font-weight: normal; text-shadow: 2px 2px 3px #AAA; box-shadow: 2px 2px 3px #AAA; border-radius: 10px; border: none; color: #FFFFFF; background: #f78a24; }
.ui-content .a-query .icon-query { display: inline-block; margin-right: 8px; width: 30px; height: 30px; vertical-align: middle; background: url("//cdn.kuaidi100.com/images/smart/icon.png") no-repeat; }
.ui-content .a-query .icon-query-transition { -webkit-transition: 30s linear; -o-transition: 30s linear; -moz-transition: 30s linear; transition: 30s linear }
.ui-content .a-query .icon-query-rotate { -webkit-transform: rotate(10800deg); -moz-transform: rotate(10800deg); -o-transform: rotate(10800deg); transform: rotate(10800deg); }
/*select*/
.smart-select-page { background: #FFFFFF }
.smart-word-list { position: fixed; top: 60px; left: 0; z-index: 5; width: 45%; height: 100%; padding-right: 18px; }
.smart-word-ul { margin: 0; padding: 0; width: 100%; height: 100%; list-style: none; background: #f3f3f3 }
.smart-word-ul li { width: 100%; height: 45px; text-align: center; background: #f7f7f7 }
.smart-word-ul li.even { background: #f3f3f3 }
.smart-word-ul li a { float: left; display: block; width: 25%; height: 45px; line-height: 45px; text-decoration: none; font-size: 20px; font-weight: bold; }
.smart-word-ul li .a-common { width: 100%; text-shadow: none; }
.smart-word-ul li.li-selected { padding-right: 18px; background: url("//cdn.kuaidi100.com/images/smart/icon.png") right -150px no-repeat }
.smart-word-ul li.li-selected a { background: #68b9f8; color: #3c87c8 }
.smart-word-ul li.li-selected a.a-selected { background: #3987c7; color: #FFFFFF }
.smart-word-ul li.li-selected a.a-common { background: #68b9f8; color: #FFFFFF }
.smart-select-list { margin-left: 45%; background: #FFFFFF }
.smart-select-list .smart-select-li { height: 35px; line-height: 35px; text-align: center; font-size: 16px; border-top: none; border-bottom: 1px solid #AAAAAA; background: #FFFFFF }
/*result*/
/*{ padding: 0 }*/
.smart-result-comname, .smart-result-kuaidinum { display: inline-block; height: 35px; line-height: 35px; text-align: center; text-shadow: none; font-size: 16px; color: #FFFFFF }
.smart-result-comname { width: 30%; padding-right: 5%; background: #88cdfe }
.smart-result-kuaidinum { width: 60%; padding-left: 5%; background: #67baf7 }
table { width: 100% }
table tr { width: 100%; font-weight: bold; color: #909090 }
table tr.last-line { color: #333333; }
table tr.last-line { color: #FF6600; }
table td { height: 60px; background: none }
table tr.odd { background: #f6f6f6 }
table tr.even { background: #eeeeee }
table td.col1 { width: 30%; }
table td.col2 { padding: 10px; width: 60%; line-height: 24px; }
table td.colstatus { width: 10% }
table tr td.colstatus { background: url("//cdn.kuaidi100.com/images/smart/step_point.png") center center no-repeat }
table tr.first-line td.colstatus { background: url("//cdn.kuaidi100.com/images/smart/step_start.png") center center no-repeat }
table tr.wait td.colstatus { background: url("//cdn.kuaidi100.com/images/smart/step_wait.gif") center center no-repeat }
table tr.checked td.colstatus { background: url("//cdn.kuaidi100.com/images/smart/step_finish.png") center center no-repeat }
.result-date, .result-time { display: block; width: 100%; text-align: center }
.result-date { height: 15px; padding-top: 10px; font-weight: normal; font-size: 12px; }
.result-time { height: 35px; font-size: 24px; }
.result-btn{padding:10px; text-align:center; background-color:#aaa;}
.result-btn a{ text-decoration:none;text-shadow:none;}
.result-btn a.ui-link, .result-btn a.ui-link:hover, .result-btn a.ui-link:visited, .result-btn a.ui-link:active {color:#fff;}

/*error*/
.smart-error-page .ui-dialog-contain { margin-top: 30%; box-shadow: none; }
.smart-error-page .smart-error-div { position: relative; padding: 0 0 40px 0; box-shadow: none; background: none }
.smart-error-content { width: 100%; height: 100%; border-radius: 10px; box-shadow: 0 0 12px rgba(0, 0, 0, .6); background: #e3750f }
.smart-error-div .icon-error { margin: 0; padding: 20px 10px 10px 70px; line-height: 30px; min-height: 50px; font-size: 20px; font-weight: bold; text-shadow: none; color: #FFFFFF; background: url("//cdn.kuaidi100.com/images/smart/icon.png") 15px -394px no-repeat }
.smart-error-div .smart-opera-btn { margin: 0 auto 20px; width: 100%; text-align: center }
.smart-error-div .btn-error-1, .smart-error-div .btn-error-2 { display: inline-block; padding-left: 30px; height: 40px; line-height: 40px; font-size: 16px; color: #333333 }
.smart-error-div .btn-error-1 { background: url("//cdn.kuaidi100.com/images/smart/icon.png") 4px -264px no-repeat #FFFFFF; width: 25%; margin-right: 10px; }
.smart-error-div .btn-error-2 { background: url("//cdn.kuaidi100.com/images/smart/icon.png") 4px -316px no-repeat #FFFFFF; width: 40%; }
.smart-error-div .btn-close { position: absolute; bottom: 0px; right: -5px; display: block; width: 28px; height: 28px; padding: 10px; border-radius: 10px; background: url("//cdn.kuaidi100.com/images/smart/icon.png") 10px -210px no-repeat #e37510; }
/* 后来增加的 快递100 部分  20120907*/
.geeku { width: 100%; height: 82px; background-color: #f1f1f1; }
.geekson { width: 292px; height: 82px; margin: 0 auto; }
.geeka { display: block; float: left; margin: 0px; padding: 0px; text-decoration: none; }
.geeka:hover { text-decoration: none; }
.glogo { float: left; text-align: left; padding: 8px 0px 0px 0px; }
.gword { float: left; padding-left: 8px; width: 198px; }
.gword01 { font-size: 18px; color: #0f0f0f; padding-top: 20px; width: 198px; }
.gword02 { font-size: 12px; color: #9f9f9f; width: 198px; float: left; padding-top: 2px; }
.gcha { float: left; padding: 26px 0px 0px 0px; }
.cha { font-size: 22px; color: #0f0f0f; display: inline-block; padding: 4px; cursor: pointer; }
.smart-footer p.adBox { height: 60px; padding-top: 10px; background-color: #fff; text-align: center; }
.result-footer { text-align: center; }
.result-footer img, .result-footer span { vertical-align: middle; }
.result-footer img { margin-right: 10px; }
/*app*/
.app-box { position: relative; width: 100%; height: 3rem; background: #f1f2f4; opacity: 1; border-top: 0.0625rem solid #d4d4d4; }
.app-open { position: relative; pointer-events: auto; height: 3rem; margin-right: 3rem; font-size: 0.875rem; }
.app-img { position: absolute; bottom: 0.25rem; left: 0.5rem; height: 2.5rem; width: 2.5rem; border: 0; }
.app-content { position: absolute; bottom: 0.25rem; left: 3.5rem; line-height: 1.25rem; color: #3f4865; }
.app-content p { color: #3e4563; height: 20px; font-size: 14px;line-height: 20px;}
.app-content p em { font-style: normal; color: #cc0000; }
.app-btn { position: absolute; bottom: 0.5rem; right: 0; display: block; width: 4rem; height: 2rem; line-height: 2rem; text-align: center; border-radius: 0.25rem; color: #fff; background: #db342a; }
.app-btn a { color: #fff }
.app-close { pointer-events: auto; position: absolute; right: 0; top: 0; height: 3rem; width: 3rem; background: url("data:image/png;base64,") center center no-repeat; background-size: 1.25rem 1.25rem; }